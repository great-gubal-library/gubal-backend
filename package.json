    "nodemon": "2.0.4",
    "ts-jest": "26.3.0",
    "tsconfig-paths": "3.9.0",
    "tslint": "6.1.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
